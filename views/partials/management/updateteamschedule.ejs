<div>
<script type="text/javascript" src="/scripts/TableWizard.js"></script>
<table class="table table-striped" id="sortableTable">
  <thead>
    <tr>
		<th scope="col"></th>
	  <th scope="col">Name</th>
      <th scope="col">Type</th>
      <th scope="col">Date</th>
	  <th scope="col">Location</th>
    </tr>
  </thead>
  <tbody>
<% if(results){ %>
	<% for(var i=0; i<results.length; i++) { %>
		 <tr>
			<td scope="row"><a href="/management/editevent?id=<%= results[i].id %>">EDIT</a></td>
			<td style="<%= results[i].cancelled ? 'color: orange;': ''%>"><%= results[i].cancelled ? results[i].name + " (cancelled)" : results[i].name %></td>
			<td><%= results[i].type %></td>
			<td><%= results[i].eventdate %></td>
			<td><%= results[i].location ? results[i].location.name : '' %></td>			
		 </tr>
	<% } %>
<% } %>
	</tbody>
</table>

</div>