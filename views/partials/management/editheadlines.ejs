<div>
<script type="text/javascript" src="/scripts/TableWizard.js"></script>
<table class="table table-striped" id="sortableTable">
  <thead>
    <tr>
		<th scope="col"></th>
	  <th scope="col" style="cursor: pointer;" onclick="sortTableAlpha(1)">Title</th>
      <th scope="col" style="cursor: pointer;" onclick="sortTableDate(2)">Date</th>
    </tr>
  </thead>
  <tbody>
<% if(results){ %>
	<% for(var i=0; i<results.length; i++) { %>
		 <tr>
			<td scope="row"><a href="/management/editHeadline?id=<%= results[i].id %>">EDIT</a></td>
			<td><%= results[i].title %></td>
			<td><%= results[i].timestamp %></td>		
		 </tr>
	<% } %>
<% } %>
	</tbody>
</table>

</div>