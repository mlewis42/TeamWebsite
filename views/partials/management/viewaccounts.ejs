<div>
<script type="text/javascript" src="/scripts/TableWizard.js"></script>
<table class="table table-striped" id="sortableTable">
  <thead>
    <tr>
		<th scope="col"></th>
      <th scope="col" style="cursor: pointer;" onclick="sortTableAlpha(1)">First</th>
      <th scope="col" style="cursor: pointer;" onclick="sortTableAlpha(2)">Last</th>
      <th scope="col" style="cursor: pointer;" onclick="sortTableAlpha(3)">Email</th>
	  <th scope="col" style="cursor: pointer;" onclick="sortTableAlpha(4)">Admin</th>
    </tr>
  </thead>
  <tbody>
<% if(results){ %>
	<% for(var i=0; i<results.length; i++) { %>
		 <tr>
			<td scope="row"><a href="/management/editAccount?id=<%= results[i].id %>">EDIT</a></td>
			<td><%= results[i].firstname %></td>
			<td><%= results[i].lastname %></td>
			<td><%= results[i].email %></td>
			<td><%= results[i].admin %></td>	
		 </tr>
	<% } %>
<% } %>
	</tbody>
</table>

</div>