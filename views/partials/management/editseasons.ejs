<div>
<script type="text/javascript" src="/scripts/TableWizard.js"></script>
<table class="table table-striped" id="sortableTable">
  <thead>
    <tr>
		<th scope="col"></th>
	  <th scope="col" style="cursor: pointer;" onclick="sortTableAlpha(1)">Name</th>
      <th scope="col" style="cursor: pointer;" onclick="sortTableNumeric(2)">Wins</th>
      <th scope="col" style="cursor: pointer;" onclick="sortTableNumeric(3)">Losses</th>
	  <th scope="col">Standing</th>
    </tr>
  </thead>
  <tbody>
<% if(results){ %>
	<% for(var i=0; i<results.length; i++) { %>
		 <tr>
			<td scope="row"><a href="/management/editSeason?id=<%= results[i].id %>">EDIT</a></td>
			<td><%= results[i].name %></td>
			<td><%= results[i].record.wins %></td>
			<td><%= results[i].record.losses %></td>
			<td><%= results[i].standing %></td>			
		 </tr>
	<% } %>
<% } %>
	</tbody>
</table>

</div>