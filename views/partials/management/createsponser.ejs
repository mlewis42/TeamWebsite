<link rel="stylesheet" type="text/css" href="/styles/createform.css" />
<div>
	<div>
		<form class="creationForm" action="/management/createsponser" enctype="multipart/form-data" method="post">
			<div class="form-group row">
				<label for="title" class="col-2 col-form-label">Sponser Name</label>
				<div class="col-10 longTextInput">
					<input type="text" class="form-control" id="title" name="name" placeholder="Name">
				</div>
			</div>
			
			<div class="form-group row">
				<label for="artcleimage" class="col-2 col-form-label">Sponser Image</label>
				<div class="col-10" style="width: 500px; height: 50px;">
					<input type="file" name="upload" multiple="multiple">
				</div>
			</div>
			
			<div class="form-group row">
				<label for="thumburl" class="col-2 col-form-label">Website</label>
				<div class="col-10 longTextInput">
					<input type="text" class="form-control" id="thumburl" name="websiteurl" placeholder="Website Url">
				</div>
			</div>
			
			<button type="submit" class="btn btn-primary row">Create Sponser</button>
		</form>
	</div>
</div>